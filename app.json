{
  "expo": {
    "name": "Ultrasound Guidance",
    "slug": "ultrasound-guidance",
    "description": "Companion app for Ultrasound Guidance learning material",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/main-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 24
          }
        }
      ],
      "expo-secure-store"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ultrasoundguidanceorg.ultrasoundguidance",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ultrasoundguidanceorg.ultrasoundguidance"
    },
    "extra": {
      "eas": {
        "projectId": "e5add483-1412-4b40-8885-326ef5ee8817"
      }
    },
    "owner": "ultrasound-guidance-org",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/e5add483-1412-4b40-8885-326ef5ee8817"
    }
  }
}
