{
  "expo": {
    "name": "Ultrasound Guidance",
    "slug": "ultrasound-guidance",
    "description": "Companion app for Ultrasound Guidance learning material",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Sk-Modernist-Regular.otf",
            "./assets/fonts/Sk-Modernist-Bold.otf"
          ]
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ultrasoundguidanceorg.ultrasoundguidance"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ultrasoundguidanceorg.ultrasoundguidance"
    },
    "extra": {
      "eas": {
        "projectId": "739696cc-40fb-4a6a-8cc4-d5c58f431fff"
      }
    },
    "owner": "ultrasound-guidance-org",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/739696cc-40fb-4a6a-8cc4-d5c58f431fff"
    }
  }
}
